// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model Mahasiswa {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  nama    String
  nik     Int    @unique
  alamat  String
  jurusan String
  ttlahir String
  email   String
  kota    String
  kodepos Int
}
